{
    "version": 2,
    "builds": [
      { "src": "**/*.html", "use": "@vercel/static" },
      { "src": "**/*.css", "use": "@vercel/static" },
      { "src": "**/*.js", "use": "@vercel/static" },
      { "src": "**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/todo", "dest": "/todo_app/todo.html" },
      { "src": "/login", "dest": "index.html" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  